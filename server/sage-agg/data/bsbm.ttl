@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
@prefix sage: <http://sage.univ-nantes.fr/sage-voc#>
@prefix foaf: <http://xmlns.com/foaf/0.1/>
@prefix dcterms: <http://purl.org/dc/terms/>
@prefix : <http://localhost:8000/>

[] a sage:SageEndpoint ;
  foaf:name "Sage testing server";
  dcterms:creator "Thomas Minier";
  sage:publicUrl <http://localhost:8000>;
  sage:quota 75;
  sage:pageSize 2000;
  sage:defaultQuery [
    a sage:ExampleQuery;
    sage:targetDataset :bsbm1M;
    foaf:name "Select everything";
    rdf:value """SELECT * WHERE {
      ?s ?p ?o.
    }""".
  ];
  sage:dataset :bsbm1M;
  sage:dataset :thomas;
  sage:dataset :arnaud.

:thomas a sage:SageDataset;
  foaf:name "thomas";
  dcterms:description "thomas' dataset";
  sage:backend sage:HDT;
  sage:HDTFile <data/graph/thomas.hdt>.

:arnaud a sage:SageDataset;
  foaf:name "arnaud";
  dcterms:description "arnaud' dataset";
  sage:backend sage:HDT;
  sage:HDTFile <data/graph/arnaud.hdt>.

:bsbm1M a sage:SageDataset;
  foaf:name "bsbm1M";
  dcterms:description "BSBM 1M triples";
  sage:backend sage:HDT;
  sage:HDTFile <data/bsbm1M.hdt>;
  sage:hasExampleQuery [
    a sage:ExampleQuery;
    foaf:name "Example query 1";
    rdf:value """
    SELECT * WHERE {
      ?s ?p ?o.
    }
    """.
  ];
  sage:hasExampleQuery [
    a sage:ExampleQuery;
    foaf:name "Example query 2";
    rdf:value """
    PREFIX bsbm-inst: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/>
    PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
    PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
    PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

    SELECT DISTINCT ?product ?label
    WHERE {
        ?product rdfs:label ?label .
        ?product a <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/ProductType66> .
        ?product bsbm:productFeature <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/ProductFeature3> .
        ?product bsbm:productFeature <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/ProductFeature1967> .
        ?product bsbm:productPropertyNumeric1 ?value1 .
      FILTER (?value1 > 136)
    }
    ORDER BY DESC(?label)
    LIMIT 10
    """.
  ].
